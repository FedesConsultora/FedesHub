    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.5",
    "pino": "^9.9.0",
    "pino-pretty": "^11.3.0",
    "sequelize": "^6.37.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
