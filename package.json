{
  "name": "fedes-hub",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "docker compose -f docker-compose.dev.yml up --build",
    "dev:down": "docker compose -f docker-compose.dev.yml down --remove-orphans",
    "dev:clean": "docker compose -f docker-compose.dev.yml down -v --remove-orphans",
    "dev:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "stack:up": "docker compose up -d",
    "prod:build": "docker compose build --no-cache",
    "prod:up": "docker compose up -d",
    "prod:logs": "docker compose logs -f"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "googleapis": "^159.0.0"
  },
  "devDependencies": {
    "clipboardy": "^5.0.0",
    "minimist": "^1.2.8"
  }
}
